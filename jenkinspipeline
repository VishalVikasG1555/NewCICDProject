pipeline
{
  agent any
  stages
  {
     stage('Git checkout')
     {
        steps
        {
           git branch: 'main', url: 'https://github.com/VishalVikasG1555/NewCICDProject.git'
        }
     }
     stage('Maven Build')
     {
        steps
        {
           sh '/opt/maven/bin/mvn clean package -Dmaven.test.skip=true'
        }
     }
      stage('running build jar file')
     {
        steps
        {
           sh 'mvn spring-boot:run'
        }
     }
  }
}


